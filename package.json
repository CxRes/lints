{
  "name": "lints",
  "version": "10.1.1",
  "description": "Lint all your JavaScript, CSS, HTML and Markdown with a single command",
  "keywords": [
    "lint",
    "linter",
    "static code analysis",
    "analysis",
    "analyser",
    "checker",
    "validation",
    "validator",
    "jslint",
    "jshint",
    "eslint",
    "csslint",
    "stylelint",
    "htmlhint",
    "w3cjs",
    "bootlint",
    "markdownlint",
    "remark-lint",
    "w3c",
    "javascript",
    "js",
    "css",
    "html",
    "markdown"
  ],
  "scripts": {
    "lint": "./bin/lints",
    "test": "tap test/*.js --cov",
    "posttest": "tap --coverage-report=text-summary && tap --coverage-report=html",
    "tap": "tap"
  },
  "bin": {
    "lints": "./bin/lints"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvgenyOrekhov/lints.git"
  },
  "author": {
    "name": "Evgeny Orekhov",
    "url": "https://plus.google.com/+ЕвгенийОрехов"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvgenyOrekhov/lints/issues"
  },
  "homepage": "https://github.com/EvgenyOrekhov/lints#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "bootlint": "^0.14.2",
    "csslint": "^1.0.5",
    "eslint": "^3.16.1",
    "globby": "^6.1.0",
    "htmlhint": "^0.9.13",
    "jshint": "^2.9.4",
    "jslinter": "^1.5.14",
    "markdownlint": "^0.3.1",
    "ramda": "^0.23.0",
    "remark": "^6.2.0",
    "remark-lint": "^5.4.0",
    "remark-preset-lint-consistent": "^1.0.0",
    "remark-preset-lint-recommended": "^1.0.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "w3cjs": "^0.3.0"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "devDependencies": {
    "eslint-config-hardcore": "^1.9.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "tap": "^10.2.2"
  }
}
